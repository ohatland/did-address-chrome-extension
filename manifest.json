{
  "name": "DID Manager",
  "version": "1.0",
  "manifest_version": 3,
  "description": "En enkel utvidelse for Ã¥ administrere og sette inn DID-adresser i inputfelt.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-64.png",
    "128": "images/icon-128.png"
  }
}